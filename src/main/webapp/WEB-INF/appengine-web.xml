<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">

    <application>chuck-bot</application>
    <version>1</version>
    <threadsafe>true</threadsafe>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties" />
        <property name="restfbmessenger.verify_token" value="${restfbmessengerVerifyToken}" />
        <property name="restfbmessenger.access_token" value="${restfbmessengerAccessToken}" />
        <property name="restfbmessenger.app_secret" value="${restfbmessengerAppSecret}" />
    </system-properties>

</appengine-web-app>
